{
  "name": "aplwindowsversion",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "The frontend and configuration interface for the APL desktop app. Based upon the work of Leo Wang's electron-vue-vite template.",
  "author": "AlooTheAloo",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run build-v && vue-tsc && vite build && electron-builder",
    "release": "bun run build-v && vue-tsc && vite build && electron-builder build -wl",
    "preview": "vite preview",
    "play": "bun --watch ./apl-backend/entry/tests.ts",
    "build-v": "cd ./apl-backend/apl-visuals && vue-tsc -b && vite build"
  },
  "devDependencies": {
    "@types/pluralize": "^0.0.33",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6",
    "@types/bun": "latest"
  },
  "dependencies": {
    "@miniben90/x-win": "^1.10.2",
    "@primevue/themes": "^4.0.5",
    "@primno/dpapi": "^1.1.2",
    "@types/node": "^22.2.0",
    "@vueuse/core": "^11.0.3",
    "axios": "^1.7.7",
    "bun": "^1.1.29",
    "chrome-cookies-secure": "^2.1.1",
    "color": "^4.2.3",
    "colors": "^1.4.0",
    "country-flag-icons": "^1.5.13",
    "dayjs": "^1.11.12",
    "electron-updater": "^6.3.4",
    "find-process": "^1.4.7",
    "fs": "^0.0.1-security",
    "get-windows": "^9.2.0",
    "int": "^0.2.0",
    "js-abbreviation-number": "^1.4.0",
    "keytar": "^7.9.0",
    "node-schedule": "^2.1.1",
    "path": "^0.12.7",
    "pickleparser": "^0.2.1",
    "pluralize": "^8.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.5",
    "puppeteer": "^23.2.1",
    "request": "^2.88.2",
    "round-to": "^6.0.0",
    "sqlite3": "^5.1.7",
    "tldjs": "^2.3.1",
    "toggl-track": "^0.8.0",
    "tough-cookie": "^4.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "url": "^0.11.4",
    "vue-router": "4"
  },
  "optionalDependencies": {
    "node-mac-permissions": "^2.4.0"
  }
}