{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["DOM", "ES2023"],
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true
  },
  "include": ["src", "apl-backend/**/*", "electron"]
}
